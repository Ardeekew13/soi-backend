{
	"compilerOptions": {
		"target": "ESNext",
		"module": "CommonJS",
		"strict": true,
		"esModuleInterop": true,
		"outDir": "dist",
		"rootDir": "src",
		"skipLibCheck": true,
		"typeRoots": ["./types", "./node_modules/@types"],
		"noImplicitAny": false,
		"types": ["node", "express", "express-session"]
	},
	"include": ["src", "src/types/**/*.d.ts"]
}
